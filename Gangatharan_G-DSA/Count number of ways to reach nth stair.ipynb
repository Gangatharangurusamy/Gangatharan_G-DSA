    "def CountNoOfways(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    elif n == 2:\n",
    "        return 2\n",
    "    ways = 0\n",
    "    for i in range(n):\n",
    "        ways = CountNoOfways(n-1) + CountNoOfways(n-2)\n",
    "    return ways\n",
    "\n",
    "\n",
    "##Driver code\n",
    "n = 4\n",
    "result = CountNoOfways(n)\n",
    "print(\"No. of Count is:\",result)"
   ]
